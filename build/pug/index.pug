        .container
            #sliderHomePage.carousel.slide(data-ride="carousel")
                ol.carousel-indicators
                    li.active(data-target='#sliderHomePage', data-slide-to='0')
                    li(data-target='#sliderHomePage', data-slide-to='1')
                .carousel-inner(role='listbox')
                    .carousel-item.active
                        img.d-block.img-fluid(src='./image/rational-slide.jpg', alt='First slide')
                    .carousel-item
                        img.d-block.img-fluid(src='./image/turbochef-slide.png', alt='Second slide')
                        .carousel-caption.d-none.d-md-block.text-left
                            a.btn.btn-secondary(href="#") VER CATALOGO
                a.carousel-control-prev(href='#sliderHomePage', role='button', data-slide='prev')
                    span.fa.fa-chevron-left(aria-hidden='true')
                    span.sr-only Previous
                a.carousel-control-next(href='#sliderHomePage', role='button', data-slide='next')
                    span.fa.fa-chevron-right(aria-hidden='true')
                    span.sr-only Next
            #listBrand.row
                .col-md-12.text-center.text-md-right
                    img.img-fluid(src="./image/moduline.png", alt="Moduline")
                    img.img-fluid(src="./image/firex.png", alt="Moduline")
                    img.img-fluid(src="./image/irinox.png", alt="Moduline")
                    img.img-fluid(src="./image/turbochef.png", alt="Moduline")
                    img.img-fluid(src="./image/electrolux.png", alt="Moduline")
                    img.img-fluid(src="./image/rational.png", alt="Moduline")
                    img.img-fluid(src="./image/welbilt.png", alt="Moduline")
                    img.img-fluid(src="./image/aligroup.png", alt="Moduline")
                    img.img-fluid(src="./image/itw.png", alt="Moduline")
                    img.img-fluid(src="./image/themiddlebycorp.png", alt="Moduline")
            #productsOfTheWeek.row
                .container__title.col-12 PRODUCTOS DE LA SEMANA
                .container__body.row
                    - for(var i=0;i<6;i++){
                    .col-md-2
                        .item__container.border.row
                            .item__discount
                                span 36% Dscto.
                            .item__brand
                                img.img-fluid(src="./image/rational.png", alt="Rational")
                            .item__img.col-md-12
                                img.img-fluid(src="./image/product01.png", alt="Producto 01")
                            .item__info.col-md-12
                                span.item__info__sencondary Manufacturer Part #:10.00.512
                                span.item__info__primary
                                    |Rational o-ring 26 x 3.5 (5 Pack)
                            .item__shopping.col-md-12.row
                                .item__shopping__price.col-md-6
                                    span.item__shopping__price__discount US$ 52.00
                                    span.item__shopping__price__current US$ 35.00
                                .item__shopping__addCart.col-md-6
                                    a.btn(href="#")
                                        |AGREGAR
                                        i.fa.fa-shopping-cart
                    - }
            #newsSection.row
                .col-sm-6
                    img.img-fluid(src="./image/noticia1.png", alt="Noticia 1")
                .col-sm-6
                    img.img-fluid(src="./image/noticia2.png", alt="Noticia 1")
            #productsInStock.row
                .container__title.col-12 PRODUCTOS DISPONIBLES
                .container__body.row
                    - for(var i=0;i<6;i++){
                    .col-md-4
                        .item__container.border.row
                            .item__discount
                                span 36% Dscto.
                            .item__brand
                                img.img-fluid(src="./image/rational.png", alt="Rational")
                            .col-md-6
                                .item__img
                                    img.img-fluid(src="./image/product01.png", alt="Producto 01")
                                .item__shopping__addCart
                                    a.btn(href="#")
                                        |AGREGAR
                                        i.fa.fa-shopping-cart
                            .col-md-6
                                .item__info
                                    span.item__info__sencondary Manufacturer Part #:10.00.512
                                    span.item__info__primary
                                        |Rational o-ring 26 x 3.5 (5 Pack)
                                .item__shopping__price
                                    span.item__shopping__price__discount US$ 52.00
                                    span.item__shopping__price__current US$ 35.00
                    - }
            #OurBlog.row
                .container__title.col-12 NUESTRO BLOG
                .container__body.row
                    #sliderOurBlog.carousel.slide(data-ride="carousel")
                        .carousel-inner(role='listbox')
                            .carousel-item.active
                                .row
                                    - for(var i=0;i<4;i++){
                                    .col-3
                                        .sliderOurBlog__item
                                            .sliderOurBlog__item__img
                                                img.img-fluid(src="./image/blog1.png", alt="")
                                            .sliderOurBlog__item__title
                                                span Rational presenta el nuevo SelfCooking Center
                                            .sliderOurBlog__item__date
                                                span 23 Febrero 2017
                                    - }
                            .carousel-item
                                .row
                                    - for(var i=0;i<4;i++){
                                    .col-3
                                        .sliderOurBlog__item
                                            .sliderOurBlog__item__img
                                                img.img-fluid(src="./image/blog2.png", alt="")
                                            .sliderOurBlog__item__title
                                                span La Selección y la comida peruana unidas en un comercial de Marca Perú
                                            .sliderOurBlog__item__date
                                                span 21 Abril 2017
                                    - }
                        a.carousel-control-prev(href='#sliderOurBlog', role='button', data-slide='prev')
                            span.fa.fa-chevron-left(aria-hidden='true')
                            span.sr-only Previous
                        a.carousel-control-next(href='#sliderOurBlog', role='button', data-slide='next')
                            span.fa.fa-chevron-right(aria-hidden='true')
                            span.sr-only Next
